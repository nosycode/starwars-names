{
  "name": "@nosycode/starwars-names",
  "version": "0.0.0-development",
  "description": "Get random Star Wars names",
  "main": "dist/index.js",
  "scripts": {
    "commit": "git-cz",
    "coverage:test": "nyc --reporter lcov npm test",
    "coverage:check": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "prebuild": "rimraf dist",
    "build": "babel src --out-dir dist --copy-files --no-copy-ignored --ignore \"src/**/*.test.js\"",
    "test": "mocha src/index.test.js --require @babel/register",
    "watch:test": "npm test -- -w",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nosycode/starwars-names.git"
  },
  "keywords": [
    "random",
    "star",
    "wars"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "author": "nosycode <nosycode@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nosycode/starwars-names/issues"
  },
  "homepage": "https://github.com/nosycode/starwars-names#readme",
  "dependencies": {
    "unique-random-array": "2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@babel/preset-env": "7.9.5",
    "@babel/register": "7.9.0",
    "chai": "4.2.0",
    "codecov": "3.6.5",
    "commitizen": "4.0.4",
    "cz-conventional-changelog": "3.1.0",
    "ghooks": "2.0.4",
    "mocha": "7.1.1",
    "nyc": "15.0.1",
    "rimraf": "3.0.2",
    "semantic-release": "17.0.6"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run coverage:test && npm run coverage:check"
    },
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  }
}
