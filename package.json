{
  "name": "@nosycode/starwars-names",
  "version": "0.0.0-development",
  "description": "Get random Star Wars names",
  "main": "src/index.js",
  "scripts": {
    "commit": "git-cz",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "test": "nyc -x \"**/*.test.js\" --r lcov  --r text-summary mocha src/index.test.js && npm run check-coverage",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nosycode/starwars-names.git"
  },
  "keywords": [
    "random",
    "star",
    "wars"
  ],
  "author": "nosycode <nosycode@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nosycode/starwars-names/issues"
  },
  "homepage": "https://github.com/nosycode/starwars-names#readme",
  "dependencies": {
    "unique-random-array": "2.0.0"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "commitizen": "4.0.4",
    "cz-conventional-changelog": "3.1.0",
    "ghooks": "2.0.4",
    "mocha": "7.1.1",
    "nyc": "15.0.1"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm test"
    },
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
